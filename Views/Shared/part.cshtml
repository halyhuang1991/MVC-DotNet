    <thead>
        <tr>
            <th>id</th>
            <th>name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model) {
        <tr>
            <td>@item.id</td>
            <td>@item.name</td>
        </tr>}
        <tr>
            <td></td>
        </tr>
    </tbody>
</table>

